{
  "name": "collaborate2",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "predev": "rimraf dist && webpack",
    "dev":
      "concurrently 'npm run dev:build' 'cross-env NODE_ENV=development node dist/index.bundle.js'",
    "dev:build": "webpack -w && npm run build:next",
    "build:next": "cd \"src/app\" && next build",
    "build": "rimraf dist && webpack && npm run build:next",
    "start:prod": "npm run build && npm run start",
    "start": "cross-env NODE_ENV=production node dist/index.bundle.js",
    "analyze": "cross-env ANALYZE=1 npm run build"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "compression": "^1.7.0",
    "core-js": "^2.5.0",
    "cross-env": "^5.0.1",
    "express": "^4.16.2",
    "express-session": "^1.15.5",
    "helmet": "^3.8.0",
    "next": "^4.2.1",
    "next-redux-wrapper": "^1.3.2",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.5",
    "react-swipeable-views": "^0.12.8",
    "react-swipeable-views-utils": "^0.12.8",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-offline": "^2.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "styled-components": "^2.2.4",
    "sw-precache-webpack-plugin": "^0.11.4",
    "webpack-bundle-analyzer": "^2.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-import": "^1.4.0",
    "babel-plugin-styled-components": "^1.1.5",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-config-prettier": "2.1.1",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-flow": "2.29.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-prettier": "2.1.1",
    "eslint-plugin-react": "7.0.1",
    "flow-bin": "^0.53.1",
    "morgan": "^1.8.2",
    "nodemon": "^1.11.0",
    "prettier": "^1.9.1",
    "rimraf": "^2.6.1",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  }
}
