{
  "presets": [
    "next/babel",
    [
      "env",
      {
        "targets": {
          "node": "6.10"
        }
      }
    ]
  ],
  "plugins": [
    "transform-flow-strip-types",
    [
      "styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ],
    ["transform-object-rest-spread"]
  ]
}
